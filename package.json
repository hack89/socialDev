    "dependencies": {
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.18.2",
        "config": "^3.2.4",
        "express": "^4.16.3",
        "express-validator": "^6.3.0",
        "gravatar": "^1.8.0",
        "jsonwebtoken": "^9.0.0",
        "mongoose": "^5.0.12",
        "passport": "^0.4.0",
        "passport-jwt": "^4.0.0",
        "request": "^2.88.0",
        "uuid": "^3.3.3",
        "validator": "^9.4.1"
    },
    "devDependencies": {
        "concurrently": "^5.0.1",
        "nodemon": "^1.17.3"
    }
}